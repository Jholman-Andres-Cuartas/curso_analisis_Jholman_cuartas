SELECT * FROM curso_analisis.animales;
CREATE TABLE `animales` (
  `id animales` int NOT NULL,
  `especie` varchar(45) COLLATE utf8mb3_spanish_ci DEFAULT NULL,
  `color` varchar(45) COLLATE utf8mb3_spanish_ci DEFAULT NULL,
  `edad` varchar(45) COLLATE utf8mb3_spanish_ci DEFAULT NULL,
  `pais` varchar(45) COLLATE utf8mb3_spanish_ci DEFAULT NULL,
  PRIMARY KEY (`id animales`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3 COLLATE=utf8mb3_spanish_ci